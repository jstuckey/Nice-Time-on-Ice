<section id="misc-links">
  <h3>
    Various Links
  </h3>
  <ul id="misc-link-list">
  <% @misc_link_presenter.links \
    .zip(@misc_link_presenter.headers) \
    .each do |link, header| %>
    <% if header.present? %>
      <h4><%= header %></h4>
    <% end %>
    <li>
      <%= link %>
    </li>
  <% end %>
  </ul>
</section>
