<section id="game-links">
  <h3>
  <span id="game-number"><%= @context.game.try(:game_number) %></span>
    By Game
  </h3>
  <ul id="game-link-list">
  <% @game_link_presenter.links \
    .zip(@game_link_presenter.headers) \
    .each do |link, header| %>
    <% if header.present? %>
      <h4><%= header %></h4>
    <% end %>
    <li>
      <%= link %>
    </li>
  <% end %>
  </ul>
</section>
